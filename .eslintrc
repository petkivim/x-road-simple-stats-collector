{
 'root': true,
 'env': {
  'es6': true,
  'node': true,
  'browser': false
 },
 'extends': [ 'airbnb', 'plugin:import/warnings' ],
 'rules': {
    'semi': ['error', 'never'],
    'no-multi-assign': 'off',
    'no-console': 'off'
  },
 'parserOptions': {
  'ecmaVersion': 6,
  'sourceType': 'module',
  'ecmaFeatures': {
   'jsx': false,
   'experimentalObjectRestSpread': true
  }
 }
}
